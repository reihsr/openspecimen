<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class
    name="com.krishagni.catissueplus.core.common.domain.UserNotification"
    table="OS_USER_NOTIFICATIONS">
	
    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_NOTIFICATIONS_SEQ</param>
      </generator>
    </id>

    <many-to-one name="user" column="USER_ID" lazy="false" fetch="join"/>
    
    <many-to-one name="notification" column="NOTIFICATION_ID" lazy="false" fetch="join"/>
    
    <property name="status" column="STATUS"/>
  </class>
</hibernate-mapping>
